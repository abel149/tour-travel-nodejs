<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href=" https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
    />

    <title>admin</title>
    <style>
      .list {
        display: none;
      }
      .feed {
        display: none;
      }
    </style>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
      <div class="container-fluid">
        <a class="navbar-brand" href="#">Joy tour</a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="#">create</a>
            </li>
            <li class="nav-item" onclick="">
              <a
                class="nav-link"
                href="#"
                onclick="
                document.querySelector('.list').style.display = 'block';
                document.querySelector('.feed').style.display = 'none';
                "
              >
                booked
              </a>
            </li>
            <li class="nav-item dropdown">
              <a
                class="nav-link dropdown-toggle"
                href="#"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
              >
                list
              </a>
              <ul class="dropdown-menu">
                <li><a class="dropdown-item" href="#">blogs</a></li>
                <li><a class="dropdown-item" href="#">manege</a></li>
                <li><hr class="dropdown-divider" /></li>
                <li>
                  <a class="dropdown-item" href="#">help</a>
                </li>
              </ul>
            </li>
            <li class="nav-item">
              <a
                class="nav-link"
                href="#"
                onclick="
              document.querySelector('.feed').style.display = 'block';
              document.querySelector('.list').style.display = 'none';
              "
                >feedbacks</a
              >
            </li>
          </ul>
          <form class="d-flex" role="search">
            <input
              class="form-control me-2"
              type="search"
              placeholder="Search"
              aria-label="Search"
            />
            <button class="btn btn-outline-success" type="submit">
              Search
            </button>
          </form>
        </div>
      </div>
    </nav>

    <!--bood peoples list-->
    <ul class="list">
      <% result.forEach (element => { %>

      <li><%- element.name%></li>
      <li><%- element.email%></li>
      <% }) %>
    </ul>
    <!--feedback from clints-->
    <ul class="feed">
      <% response.forEach (element => { %>

      <li><%- element.name%></li>
      <li><%- element.email%></li>
      <li><%- element.feed%></li>
      <% }) %>
    </ul>
    <script src="	https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
